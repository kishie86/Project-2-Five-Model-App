<%= form_for @resource do |f| %>
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, User.all, :id, :username  %>
    <br>
    <%= f.label :website_domain %>
    <%= f.text_field :website_domain %>
    <br>
    <%= f.label :notes %>
    <%= f.text_area :notes%>
    <br>
    <%= f.submit%>
    <% end %>
