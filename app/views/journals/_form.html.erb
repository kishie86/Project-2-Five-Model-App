<%= form_for @journal do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :date %>
    <%= f.text_area :date %>

    <%= f.label :title %>
    <%= f.text_area :title %>

    <%= f.label :content %>
    <%= f.text_area :content %>
    
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, User.all, :id, :username %>
    
    <%= f.submit %>
  <% end %>